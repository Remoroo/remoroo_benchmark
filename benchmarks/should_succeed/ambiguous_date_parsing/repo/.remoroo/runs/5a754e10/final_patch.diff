+import pandas as pd
+
+# Read the data.csv file
+# We must respect the 'region' column for date parsing
+
+def parse_date(row):
+    date_str = row["date"]
+    region = row["region"]
+    # US: MM/DD/YYYY, EU: DD/MM/YYYY, fallback: ISO
+    if region == "US":
+        try:
+            return pd.to_datetime(date_str, format="%m/%d/%Y")
+        except Exception:
+            return pd.to_datetime(date_str, errors="coerce")
+    elif region == "EU":
+        try:
+            return pd.to_datetime(date_str, format="%d/%m/%Y")
+        except Exception:
+            return pd.to_datetime(date_str, errors="coerce")
+    else:
+        return pd.to_datetime(date_str, errors="coerce")
+
+def main():
+    df = pd.read_csv("data.csv")
+    df["parsed_date"] = df.apply(parse_date, axis=1)
+    jan2023 = df[(df["parsed_date"].dt.year == 2023) & (df["parsed_date"].dt.month == 1)]
+    total_sales = float(jan2023["sales"].sum())
+    with open("result.txt", "w") as f:
+        f.write(str(total_sales))
+
+if __name__ == "__main__":
+    main()
